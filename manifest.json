{
    "manifest_version": 2,
    "name": "Scramble",
    "version": "1.0",

    "description": "Scrambles the text on a web page",

    "icons": {
      "48": "icons/IG_Grey-48.png"
    },

    "permissions": [
        "activeTab"
    ],

    "browser_action": {
        "default_icon": {
            "16": "icons/IG_Grey-16.png",
            "32": "icons/IG_Grey-32.png"
        },
        "default_title": "Scramble",
        "default_popup": "popup/menu.html"
    }
  }